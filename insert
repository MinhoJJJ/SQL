insert wat_db                       // 내용을 추가할 데이터베이스 이름, 여기서 컬럼들이 다 순서대로 잘 있다면 컬럼 생략 가능
select                              // 보는 기능의 select 구문 작성
       '박씨'                        // 박씨라는 사람이 추가가 되었으로 이름 부분에 '박씨'라고 작성
       a.customer                   // 김씨의 고객리스트에 해당   
  from wat_db a                     // 정보를 어디서 가져올 것인지에서 wat_db를 선택하고 이름을 a로 지정하였다.
                                   
where name='김씨';                   // wat_db속성의 a에서 '김씨'라는 사람의 컬럼값들을 가져오게한다. a.customer은
                                    // 김씨.customer 같은 느낌
