CREATE OR REPLACE FUNCTION PLUS_STRING555 
(
	NAMES  IN  VARCHAR2
)
RETURN VARCHAR2
IS
	NEW_NAME VARCHAR2(50);
	BEGIN
	NEW_NAME:=CONCAT(NAMES,'SSS');
	RETURN NEW_NAME;
END;


CREATE OR REPLACE FUNCTION SUBSTR_STRING
(
	NAME  IN  VARCHAR2
)
RETURN VARCHAR2
IS
	NEW_NAME VARCHAR2(50);
	
	BEGIN
	NEW_NAME:=SUBSTR(NAME,1,3)|| '****'|| SUBSTR(NAME,9,11) ;
	RETURN NEW_NAME;
END;




SELECET PULS_STRING555('5555')
FROM DUAL;
